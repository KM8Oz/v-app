{
  "name": "vignettes",
  "version": "1.0.0",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Fast Refresh for rapid application development",
  "main": "src/main/bundle.js",
  "author": {
    "name": "kmoz000",
    "email": "ouazmir@gmail.com",
    "url": "https://kmoz.dev"
  },
  "license": "UNLICENCED",
  "scripts": {
    "start": "concurrently -n=react,electron -c=blue,green \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "node scripts/render-start",
    "dev:electron": "node scripts/main-build --env=development --watch",
    "build-win": "node scripts/render-build && node scripts/main-build --env=production && electron-builder -wl",
    "build-mac": "node scripts/render-build && node scripts/main-build --env=production && electron-builder -m"
  },
  "build": {
    "productName": "Facturation des vignettes",
    "appId": "ma.dup.vignettes",
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "files": [
      "assets/**/*",
      "!node_modules",
      "src/main",
      "src/dist"
    ],
    "mac": {
      "icon": "assets/icon.icns",
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "extends": null,
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-app/v-app.git"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^4.2.2",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "localforage": "^1.10.0",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "mobx-state-tree": "^5.0.2",
    "mst-persist": "^0.1.3",
    "os-utils": "^0.0.14",
    "react": "^16.13.1",
    "react-barcodes": "^1.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^16.13.1",
    "react-number-format": "^4.7.3",
    "react-router-dom": "^5.1.2",
    "react-select": "^4.3.1",
    "react-spring": "^9.2.4",
    "socket.io-client": "^4.2.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/crypto-js": "^4.0.2",
    "@types/react": "^16.9.34",
    "@types/react-date-range": "^1.4.1",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^4.0.17",
    "@types/styled-components": "^5.1.14",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "chalk": "^4.0.0",
    "chokidar": "^3.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "electron": "^9.0.0-beta.24",
    "electron-builder": "^22.6.0",
    "electron-connect": "^0.6.3",
    "electron-is-dev": "^1.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "minimist": "^1.2.5",
    "ora": "^4.0.4",
    "style-loader": "^1.2.1",
    "wait-on": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "contributors": [
    {
      "name": "kmoz000",
      "email": "kimo@oldi.dev",
      "url": "https://github.com/km8oz"
    }
  ]
}
